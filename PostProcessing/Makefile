# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  PostProcessing_config = debug

else ifeq ($(config),release)
  PostProcessing_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := PostProcessing

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

PostProcessing:
ifneq (,$(PostProcessing_config))
	@echo "==== Building PostProcessing ($(PostProcessing_config)) ===="
	@${MAKE} --no-print-directory -C PostProcessing -f Makefile config=$(PostProcessing_config)
endif

clean:
	@${MAKE} --no-print-directory -C PostProcessing -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   PostProcessing"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"